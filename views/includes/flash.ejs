<% if(success && success.lenght) { %>
<div class="alert alert-success" role="alert">
    <%= success%>
<button
 type="button"
 class="btn-close"
 data-bs-dismiss="alert"
 aria-label="close"></button>
</div>
<% } %>
<% if(error && error.lenght) { %>
<div class="alert alert-danger " role="alert">
    <%= error%>
<button
 type="button"
 class="btn-close"
 data-bs-dismiss="alert"
 aria-label="close"></button>
</div>
<% } %>